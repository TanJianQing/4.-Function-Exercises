def ordinal(number):
    """
    Returns the ordinal number based on the integer
    """
    ordinal_list = ['first', 'second', 'third', 'fourth', 'fifth', 'sixth', 'seventh', 'eight', 'ninth',
                    'tenth', 'eleventh', 'twelfth']

    # Return the ordinal number for numbers from 1 to 12 inclusive
    if number in range(1,len(ordinal_list)+1):
        return ordinal_list[number-1]

    # Returns blank string otherwise
    else:
        return ''

def main():
    """
    Prints all ordinal based on integers from 1 to 12 inclusive
    """
    for i in range(1,13):
        print(f'{i} is {ordinal(i)}')


if __name__ == '__main__':
    main()
